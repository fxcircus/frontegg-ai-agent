{
  "name": "commitment-lifecycle-agent",
  "version": "1.0.0",
  "description": "A demonstration project showcasing how to build AI agents using Frontegg AI framework SDK for authentication, third-party integrations, and user context management. Features a B2B agent that manages product commitments through Slack, Jira, HubSpot, and Google Calendar integrations.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev:fe": "npx vite",
    "build:fe": "tsc && vite build",
    "preview:fe": "vite preview",
    "start": "node dist/index.js",
    "start:server": "tsx src/server.ts",
    "dev:server": "tsx watch src/server.ts",
    "dev:all": "concurrently \"npm:dev:server\" \"npm:dev:fe\"",
    "test": "jest",
    "test:simple": "ts-node src/test.ts",
    "lint": "eslint src --ext .ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@frontegg/ai-sdk": "^2.0.0",
    "@frontegg/react": "^7.7.0",
    "@langchain/community": "^0.3.40",
    "@langchain/core": "^0.3.44",
    "@langchain/langgraph": "^0.2.64",
    "@langchain/mcp-adapters": "^0.4.2",
    "@langchain/openai": "^0.5.5",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@modelcontextprotocol/server-slack": "^2025.1.17",
    "@slack/web-api": "^7.9.1",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "eventsource": "^2.0.2",
    "express": "^4.21.2",
    "framer-motion": "^12.6.3",
    "langchain": "^0.3.21",
    "openai": "^4.94.0",
    "patch-package": "^8.0.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.5.1",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.3.0",
    "true-myth": "^8.5.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vite": "^6.3.2"
